{
    "name": "dev-kayes-server",
    "version": "1.0.0",
    "main": "server.js",
    "type": "module",
    "author": "kayes",
    "private": true,
    "license": "MIT",
    "description": "This Project contain server side code for Dev Kayes (Documental Portfolio) using node ts",
    "repository": "",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules --no-notify --exit-child server.ts",
        "start": "node server.js",
        "build": "tsc",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint --fix . --ext .ts"
    },
    "eslintConfig": {
        "extends": [
            "eslint:recommended",
            "plugin:@typescript-eslint/recommended",
            "prettier"
        ],
        "parser": "@typescript-eslint/parser",
        "plugins": [
            "@typescript-eslint",
            "prettier"
        ],
        "rules": {
            "prettier/prettier": "error"
        }
    },
    "dependencies": {
        "@total-typescript/ts-reset": "^0.4.2",
        "@types/cookie-parser": "^1.4.3",
        "@types/express": "^4.17.17",
        "@types/jsonwebtoken": "^8.5.1",
        "@types/morgan": "^1.9.4",
        "@types/node": "^16.11.7",
        "axios": "^0.24.0",
        "bcryptjs": "^2.4.3",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "date-fns": "^2.29.3",
        "dotenv": "^16.0.3",
        "express": "^4.17.1",
        "express-rate-limit": "^5.3.0",
        "express-validator": "^7.0.1",
        "helmet": "^4.6.0",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.29.4",
        "mongoose": "^6.1.4",
        "morgan": "^1.10.0",
        "nodemon": "^2.0.15"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.6.1",
        "@typescript-eslint/parser": "^5.6.0",
        "eslint": "^8.3.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-config-standard-with-typescript": "^19.0.1",
        "eslint-plugin-import": "^2.24.2",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-promise": "^5.1.0",
        "prettier": "^2.4.1",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.4.4"
    }
}
